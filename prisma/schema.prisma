generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["postgresqlExtensions"]
}

datasource db {
  provider   = "postgresql"
  url        = env("DATABASE_URL")
  extensions = [vector]
}

model DocumentSection {
  id        String                       @id
  content   String
  embedding Unsupported("vector(1536)")?
  metadata  Json?
  source    String

  @@index([source])
}
